{
  "name": "Alchemy Spotify Player",
  "description": "Connects Alchemy Scenes to Spotify Playlists",
  "version": "0.1.0",
  "manifest_version": 3,
  "key": "",
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "assets/alchemy-spotify.png"
  },
  "permissions": ["storage", "scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://app.alchemyrpg.com/game/*"],
      "js": ["content/content.js"]
    }
  ]
}
